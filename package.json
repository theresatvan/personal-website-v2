{
  "name": "personal-website-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prettier --check . && stylelint --allow-empty-input \"**/*.{css,scss}\" && next build",
    "build:local": "prettier --write . && stylelint --allow-empty-input \"**/*.{css,scss}\" && next build",
    "start": "next start",
    "lint": "prettier --check . && stylelint --allow-empty-input \"**/*.{css,scss}\" && next lint",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.22.10",
    "contentful": "^10.15.0",
    "graphql-request": "^7.1.0",
    "next": "14.2.5",
    "nodemon": "^3.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.8.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "tailwindcss": "^3.4.1"
  }
}
